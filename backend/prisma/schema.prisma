generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  username String? @unique
  password String
  address  String
  pin      BigInt  @db.BigInt
  gMapLink String
}

model Menu {
  id       String @id @default(uuid())
  category String
  itemName String
  itemDesc String
  price    Float
}
